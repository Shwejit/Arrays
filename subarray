#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int arr[10];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int sum=0;
    for(int i=0;i<n;i++)
    {
        sum=0;
        for(int j=i;j<n;j++)
        {
            sum+=arr[j];

            for(int k=i;k<=j;k++)
            {
                
                cout<<arr[k]<<" ";
            }
            cout<<sum;
            cout<<endl;
            
        }
        cout<<endl;
        
    }

    return 0;
}
